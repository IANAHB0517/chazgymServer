generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model USER {
  ID                   String   @id @db.Char(10)
  ISGROUP              String?  @default("N") @db.Char(1)
  BUSINESSTYPE         String?  @db.Char(1)
  GUARDIANNAME         String   @db.VarChar(26)
  GUARDIANRELATIONSHIP String?  @db.VarChar(10)
  GUARDIANPHONE        String   @db.Char(11)
  PHONE                String   @db.Char(11)
  MEMOFORCOACH         String?  @db.Text
  MEMO                 String?  @db.Text
  MEMOFORADMIN         String?  @db.Text
  BIRTHDAY             String   @db.Char(8)
  GENDER               String   @db.Char(1)
  ZIPCODE              String   @db.Char(5)
  ADDR                 String?  @db.VarChar(255)
  DETAILADDR           String?  @db.VarChar(255)
  GRADE                String   @db.Char(1)
  STATE                String   @db.Char(1)
  REGADMINID           String   @db.Char(10)
  MODADMINID           String   @db.Char(10)
  REGDT                DateTime @default(now()) @db.DateTime(0)
  MODDT                DateTime @default(now()) @db.DateTime(0)
}
